# Discord Bot Configuration
DISCORD_TOKEN=your_bot_token_here
CLIENT_ID=your_client_id_here
GUILD_ID=your_guild_id_here

# Web Server Configuration
WEB_SERVER_PORT=4050
WEB_DASHBOARD_ENABLED=true

# Bot Settings
BOT_PREFIX=!
PROJECT_NAME=my-discord-bot

# Feature Flags
ENABLE_WELCOME_MESSAGES=true
ENABLE_AUTO_MODERATION=false
ENABLE_LOGGING=true

# Channel IDs (Optional)
LOGS_CHANNEL_ID=
WELCOME_CHANNEL_ID=
COMMANDS_CHANNEL_ID=

# Role IDs (Optional)
ADMIN_ROLE_ID=
MODERATOR_ROLE_ID=
MEMBER_ROLE_ID=

# Database Path
DATABASE_PATH=./data